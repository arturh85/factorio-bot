{
  "name": "factorio-bot",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run tauri:serve",
    "test:unit": "jest",
    "lint": "eslint",
    "serve": "vite --port 8080",
    "native:serve": "cross-env VITE_HTTP_HANDLER=NATIVE vite --port 8080",
    "build:web": "vite build",
    "tauri:serve": "concurrently -r -n \"rust,VITE\" -k \"tauri dev\" \"npm run native:serve\"",
    "tauri:serve:release": "concurrently -r -n \"rust,VITE\" -k \"tauri dev --release\" \"npm run native:serve\"",
    "tauri:build": "cross-env VITE_HTTP_HANDLER=NATIVE vite build && tauri build --verbose",
    "test:ci": "npm run lint && npm run test:unit && npm run tauri:build && npm run cargo:test && npm run cargo:clippy",
    "cargo:clippy": "cd .. && cargo clippy",
    "cargo:test": "cd .. && cargo test",
    "cargo:tree": "cd .. && cargo tree --charset ascii > tree.txt",
    "cargo:update": "cd .. && cargo update",
    "cargo:outdated": "cd .. && cargo outdated",
    "cargo:docs": "cd .. && cargo doc --all --no-deps && cd docs && mdbook build && cd .. && del index.html",
    "changelog:install": "cargo install git-cliff",
    "changelog:update": "cd .. && git cliff --output CHANGELOG.md",
    "deps:update": "npm-check-updates -u && yarn && npm run cargo:update",
    "version:update-from-cargo": "node update-package-version.js",
    "checksum:update": "node update-checksum.js",
    "dummy": "true"
  },
  "dependencies": {
    "@tauri-apps/api": "1.0.0-beta.8",
    "@vueuse/core": "^6.2.1",
    "ansi-html": "^0.0.7",
    "chart.js": "3.5.1",
    "d3": "^7.0.1",
    "monaco-editor": "^0.27.0",
    "pinia": "^2.0.0-alpha.7",
    "prettier": "^2.3.2",
    "primeflex": "3.0.1",
    "primeicons": "4.1.0",
    "primevue": "^3.7.1",
    "prismjs": "^1.24.1",
    "vue": "^3.2.1",
    "vue-global-api": "0.4.1",
    "vue-router": "4.0.10",
    "ynetwork": "3.0.5"
  },
  "devDependencies": {
    "@tauri-apps/cli": "1.0.0-beta.10",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "@vitejs/plugin-vue": "1.6.1",
    "@vue/compiler-sfc": "3.2.9",
    "@vuedx/typecheck": "^0.7.4",
    "@vuedx/typescript-plugin-vue": "^0.7.4",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.17.0",
    "jest": "^27.1.0",
    "rollup-plugin-visualizer": "^5.5.2",
    "sass": "1.39.0",
    "sass-loader": "12.1.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "~4.4.2",
    "vite": "2.5.3",
    "vue-jest": "^5.0.0-0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "license": "MIT"
}
