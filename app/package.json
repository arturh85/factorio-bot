{
  "name": "factorio-bot",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "yarn run tauri:serve",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "tsc --noEmit && vue-tsc --noEmit && eslint src/",
    "serve": "vite --port 8080",
    "native:serve": "cross-env VITE_HTTP_HANDLER=NATIVE vite --port 8080",
    "native:start": "tauri dev",
    "build:web": "vite build",
    "tauri:keygen": "tauri signer generate -w ../../factorio-bot.key",
    "tauri:serve": "concurrently -r -n \"rust,VITE\" -k \"yarn tauri dev\" \"yarn run native:serve\"",
    "tauri:serve:release": "concurrently -r -n \"rust,VITE\" -k \"yarn tauri dev --release\" \"yarn run native:serve\"",
    "tauri:dev": "tauri dev",
    "tauri:build": "cross-env VITE_HTTP_HANDLER=NATIVE vite build && tauri build",
    "test:ci": "yarn run lint && yarn run test:unit && yarn run tauri:build && yarn run cargo:test && yarn run cargo:clippy",
    "cargo:clippy": "cd .. && cargo clippy --workspace --all-features --all-targets -- --deny warnings --deny deprecated",
    "cargo:test": "cd .. && cargo test",
    "cargo:tree": "cd .. && cargo tree --charset ascii > tree.txt",
    "cargo:docs": "cd .. && cargo doc --all --no-deps",
    "cargo:userguide": "cd ../docs/userguide && mdbook serve -o",
    "cargo:devguide": "cd ../docs/devguide && mdbook serve -o",
    "changelog:update": "cd .. && git cliff --output CHANGELOG.md --config Cargo.toml",
    "precommit:check": "yarn run lint && yarn run test:coverage && yarn run cargo:clippy && yarn run cargo:test && cd .. && cargo build --all-features && cargo build --no-default-features",
    "dummy": "true"
  },
  "dependencies": {
    "@babel/types": "7.22.5",
    "@tauri-apps/api": "1.2.0",
    "@vueuse/core": "10.2.0",
    "ansi-html": "0.0.9",
    "mermaid": "10.2.3",
    "monaco-editor": "0.39.0",
    "nanoid": "4.0.2",
    "pinia": "2.1.4",
    "primeflex": "3.3.1",
    "primeicons": "6.0.1",
    "primevue": "3.29.2",
    "vue": "3.3.4",
    "vue-router": "4.2.2",
    "ynetwork": "3.0.6"
  },
  "devDependencies": {
    "@tauri-apps/cli": "1.2.3",
    "@types/node": "18.16.18",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/compiler-sfc": "3.3.4",
    "c8": "8.0.0",
    "concurrently": "8.2.0",
    "cross-env": "7.0.3",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "9.15.0",
    "prettier": "2.8.8",
    "rollup": "3.25.1",
    "rollup-plugin-visualizer": "5.9.2",
    "sass": "1.64.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.3",
    "vite": "4.3.9",
    "vite-plugin-monaco-editor": "1.0.12",
    "vitest": "0.21.1",
    "vue-tsc": "1.8.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "license": "MIT"
}
