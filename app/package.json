{
  "name": "factorio-bot",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run tauri:serve",
    "test:unit": "jest",
    "lint": "eslint",
    "serve": "vite --port 8080",
    "native:serve": "cross-env VITE_HTTP_HANDLER=NATIVE vite --port 8080",
    "native:start": "tauri dev",
    "build:web": "vite build",
    "tauri:serve": "concurrently -r -n \"rust,VITE\" -k \"tauri dev\" \"npm run native:serve\"",
    "tauri:serve:release": "concurrently -r -n \"rust,VITE\" -k \"tauri dev --release\" \"npm run native:serve\"",
    "tauri:build": "cross-env VITE_HTTP_HANDLER=NATIVE vite build && tauri build --verbose",
    "test:ci": "npm run lint && npm run test:unit && npm run tauri:build && npm run cargo:test && npm run cargo:clippy",
    "cargo:clippy": "cd .. && cargo clippy --workspace --all-features --all-targets -- --deny warnings --allow deprecated",
    "cargo:test": "cd .. && cargo test",
    "cargo:tree": "cd .. && cargo tree --charset ascii > tree.txt",
    "cargo:update": "cd .. && cargo update",
    "cargo:outdated": "cd .. && cargo outdated",
    "cargo:docs": "cd .. && cargo doc --all --no-deps && cd docs && mdbook watch",
    "changelog:install": "cargo install git-cliff",
    "changelog:update": "cd .. && git cliff --output CHANGELOG.md",
    "deps:update": "npm-check-updates -u && yarn && npm run cargo:update",
    "version:update-from-cargo": "node update-package-version.js",
    "checksum:update": "node update-checksum.js",
    "dummy": "true"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.0-rc.1",
    "@vueuse/core": "^7.6.2",
    "ansi-html": "^0.0.7",
    "chart.js": "3.7.1",
    "d3": "^7.3.0",
    "pinia": "^2.0.11",
    "prettier": "^2.5.1",
    "primeflex": "3.1.2",
    "primeicons": "5.0.0",
    "primevue": "^3.12.0",
    "prismjs": "^1.26.0",
    "vue": "^3.2.31",
    "vue-global-api": "0.4.1",
    "vue-router": "4.0.12"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.0.0-rc.4",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vue/compiler-sfc": "3.2.31",
    "@vuedx/typecheck": "^0.7.4",
    "@vuedx/typescript-plugin-vue": "^0.7.4",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.4.1",
    "jest": "^27.5.1",
    "rollup-plugin-visualizer": "^5.5.4",
    "sass": "1.49.7",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "vite": "^2.8.3",
    "vue-jest": "^5.0.0-0",
    "vue-tsc": "^0.31.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "license": "MIT"
}
