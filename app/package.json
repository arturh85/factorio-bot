{
  "name": "factorio-bot",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run tauri:serve",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "tsc --noEmit && vue-tsc --noEmit && eslint src/",
    "serve": "vite --port 8080",
    "native:serve": "cross-env VITE_HTTP_HANDLER=NATIVE vite --port 8080",
    "native:start": "tauri dev",
    "build:web": "vite build",
    "tauri:keygen": "tauri signer generate -w ../../factorio-bot.key",
    "tauri:serve": "concurrently -r -n \"rust,VITE\" -k \"tauri dev\" \"npm run native:serve\"",
    "tauri:serve:release": "concurrently -r -n \"rust,VITE\" -k \"tauri dev --release\" \"npm run native:serve\"",
    "tauri:build": "cross-env VITE_HTTP_HANDLER=NATIVE vite build && tauri build",
    "test:ci": "npm run lint && npm run test:unit && npm run tauri:build && npm run cargo:test && npm run cargo:clippy",
    "cargo:clippy": "cd .. && cargo clippy --workspace --all-features --all-targets -- --deny warnings --deny deprecated",
    "cargo:test": "cd .. && cargo test",
    "cargo:tree": "cd .. && cargo tree --charset ascii > tree.txt",
    "cargo:docs": "cd .. && cargo doc --all --no-deps",
    "cargo:userguide": "cd ../docs/userguide && mdbook serve -o",
    "cargo:devguide": "cd ../docs/devguide && mdbook serve -o",
    "changelog:update": "cd .. && git cliff --output CHANGELOG.md --config Cargo.toml",
    "precommit:check": "npm run lint && npm run test:coverage && npm run cargo:clippy && npm run cargo:test && cd .. && cargo build --all-features && cargo build --no-default-features",
    "dummy": "true"
  },
  "dependencies": {
    "@babel/types": "7.18.4",
    "@tauri-apps/api": "1.0.1",
    "@vueuse/core": "8.7.3",
    "ansi-html": "0.0.9",
    "mermaid": "9.1.2",
    "monaco-editor": "0.33.0",
    "nanoid": "4.0.0",
    "pinia": "2.0.14",
    "primeflex": "3.2.1",
    "primeicons": "5.0.0",
    "primevue": "3.15.0",
    "vue": "3.2.37",
    "vue-router": "4.0.16",
    "ynetwork": "3.0.6"
  },
  "devDependencies": {
    "@tauri-apps/cli": "1.0.0",
    "@types/node": "17.0.18",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "@vitejs/plugin-vue": "2.3.3",
    "@vue/compiler-sfc": "3.2.37",
    "c8": "7.11.3",
    "concurrently": "7.2.2",
    "cross-env": "7.0.3",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "9.1.1",
    "prettier": "2.7.1",
    "rollup": "2.75.7",
    "rollup-plugin-visualizer": "5.6.0",
    "sass": "1.53.0",
    "ts-node": "10.8.1",
    "typescript": "4.7.4",
    "vite": "2.9.12",
    "vite-plugin-monaco-editor": "1.0.10",
    "vitest": "0.16.0",
    "vue-tsc": "0.38.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "license": "MIT"
}
