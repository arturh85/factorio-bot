{
  "package": {
    "productName": "factorio-bot",
    "version": "99.99.99"
  },
  "build": {
    "distDir": "../dist",
    "devPath": "http://localhost:8080",
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "withGlobalTauri": false
  },
  "tauri": {
    "bundle": {
      "active": true,
