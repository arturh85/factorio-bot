{
  "name": "factorio-bot",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test:unit": "jest",
    "lint": "eslint",
    "serve": "vite --port 8080",
    "native:serve": "cross-env VITE_HTTP_HANDLER=NATIVE vite --port 8080",
    "build:web": "vite build",
    "tauri:serve": "concurrently -k \"tauri dev\" \"npm run native:serve\"",
    "tauri:build": "cross-env VITE_HTTP_HANDLER=NATIVE vite build && tauri build",
    "test:ci": "npm run lint && npm run test:unit",
    "cargo:clippy": "cd .. && cargo clippy",
    "cargo:test": "cd .. && cargo test",
    "cargo:update": "cd .. && cargo update"
  },
  "dependencies": {
    "@mdi/font": "5.9.55",
    "@tauri-apps/api": "1.0.0-beta.5",
    "chart.js": "3.5.0",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-vue": "^7.15.0",
    "jest": "^27.0.6",
    "pinia": "^2.0.0-alpha.7",
    "primeflex": "3.0.1",
    "primeicons": "4.1.0",
    "primevue": "^3.6.0",
    "prismjs": "^1.24.1",
    "roboto-fontface": "0.10.0",
    "rollup-plugin-visualizer": "^5.5.2",
    "vue": "^3.2.0-beta.7",
    "vue-global-api": "0.2.4",
    "vue-router": "4.0.10",
    "ynetwork": "2.8.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "1.0.0-beta.6",
    "@vitejs/plugin-vue": "1.2.5",
    "@vue/compiler-sfc": "3.1.5",
    "eslint": "^7.32.0",
    "sass": "1.35.2",
    "sass-loader": "12.1.0",
    "ts-jest": "^26.0.0",
    "typescript": "~4.3.5",
    "vite": "2.4.2",
    "vue-cli-plugin-vuetify": "2.4.1",
    "vue-jest": "^5.0.0-0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "license": "MIT"
}
