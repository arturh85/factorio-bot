{
  "name": "factorio-bot",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test:unit": "jest",
    "lint": "eslint",
    "serve": "vite --port 8080",
    "native:serve": "cross-env VITE_HTTP_HANDLER=NATIVE vite --port 8080",
    "build:web": "vite build",
    "tauri:serve": "concurrently -n \"rust,VITE\" -k \"cross-env RUST_BACKTRACE=full tauri dev\" \"npm run native:serve\"",
    "tauri:build": "cross-env VITE_HTTP_HANDLER=NATIVE vite build && tauri build",
    "test:ci": "npm run lint && npm run test:unit && npm run tauri:build && npm run cargo:test && npm run cargo:clippy",
    "cargo:clippy": "cd .. && cargo clippy",
    "cargo:test": "cd .. && cargo test",
    "cargo:update": "cd .. && cargo update",
    "version:update-from-cargo": "node update-package-version.js",
    "dummy": "true"
  },
  "dependencies": {
    "@tauri-apps/api": "1.0.0-beta.5",
    "chart.js": "3.5.0",
    "pinia": "^2.0.0-alpha.7",
    "prettier": "^2.3.2",
    "primeflex": "3.0.1",
    "primeicons": "4.1.0",
    "primevue": "^3.6.0",
    "prismjs": "^1.24.1",
    "vue": "^3.2.0-beta.7",
    "vue-global-api": "0.4.1",
    "vue-router": "4.0.10",
    "ynetwork": "3.0.5"
  },
  "devDependencies": {
    "@tauri-apps/cli": "1.0.0-beta.6",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "@vitejs/plugin-vue": "1.3.0",
    "@vue/compiler-sfc": "3.1.5",
    "@vuedx/typecheck": "^0.7.4",
    "@vuedx/typescript-plugin-vue": "^0.7.4",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.15.1",
    "jest": "^27.0.6",
    "rollup-plugin-visualizer": "^5.5.2",
    "sass": "1.37.5",
    "sass-loader": "12.1.0",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "typescript": "~4.3.5",
    "vite": "2.4.4",
    "vue-cli-plugin-vuetify": "2.4.2",
    "vue-jest": "^5.0.0-0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "license": "MIT"
}